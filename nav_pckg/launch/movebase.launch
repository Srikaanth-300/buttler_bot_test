
<launch>
​ 
<arg name="base_global_planner" default="navfn/NavfnROS"/>
 <arg name="base_local_planner" default="base_local_planner/TrajectoryPlannerROS"/>

 <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

  <param name="base_global_planner" value="$(arg base_global_planner)"/>
  <param name="base_local_planner" value="$(arg base_local_planner)"/>  
  <param name="recovery_behavior_enabled" value="true"/>
  <param name="controller_frequency" value="5.0"/>
  <param name ="use_sim_time" value="true"/>

  <rosparam file="$(find nav_pckg)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
  <rosparam file="$(find nav_pckg)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
  <rosparam file="$(find nav_pckg)/config/local_costmap_params.yaml" command="load" />
  <rosparam file="$(find nav_pckg)/config/global_costmap_params.yaml" command="load" />
  <rosparam file="$(find nav_pckg)/config/base_local_planner_params.yaml" command="load" />

 </node>

​</launch>